{
  "tasks": {
    "dev": "deno run --watch main.ts",
    "browser:cache": "PUPPETEER_PRODUCT=chrome deno run -A --unstable https://deno.land/x/puppeteer@16.2.0/install.ts",
    "scrape": "deno run --check -A scrape.ts",
    "search": "deno run --check -A search.ts",
    "get": "deno task scrape && deno task search",
    "serve": "deno run --check -A --watch --unstable main.ts"
  },
  "lock": true,
  "fmt": {
    "semiColons": true
  },
  "imports": {
    "~/": "./"
  }
}
